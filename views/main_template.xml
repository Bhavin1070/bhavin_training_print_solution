<odoo>
	<data>

		<record model="ir.ui.view" id="user_list">
			<field name="name">User List</field>
			<field name="model">user.user</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" />
					<field name="email" />
					<field name="mobile" />
					<field name="address" />
					<field name="city" />
					<field name="district" />
					<field name="pincode" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="user_form">
			<field name="name">User Form</field>
			<field name="model">user.user</field>
			<field name="arch" type="xml">
				<form name="User Form">
					<sheet>
						<group>
							<field name="name" />
							<field name="email" />
							<field name="mobile" />
							<field name="address" />
							<field name="city" />
							<field name="district" />
							<field name="pincode" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.actions.act_window" id="user_action">
			<field name="name">User</field>
			<field name="res_model">user.user</field>
			<field name="view_mode">tree,form</field>
		</record>

		<record model="ir.ui.view" id="provider_list">
			<field name="name">Provider List</field>
			<field name="model">provider.provider</field>
			<field name="arch" type="xml">
				<tree>
					<field name="owner_name" />
					<field name="name" />
					<field name="gst_number" />
					<field name="email" />
					<field name="mobile" />
					<field name="address" />
					<field name="city" />
					<field name="district" />
					<field name="pincode" />
					<field name="open_time" />
					<field name="closed_time" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="provider_form">
			<field name="name">Provider Form</field>
			<field name="model">provider.provider</field>
			<field name="arch" type="xml">
				<form name="Provider Form">
					<sheet>
						<group>
							<field name="owner_name" />
							<field name="name" />
							<field name="gst_number" />
							<field name="email" />
							<field name="mobile" />
							<field name="address" />
							<field name="city" />
							<field name="district" />
							<field name="pincode" />
							<field name="open_time" />
							<field name="closed_time" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.actions.act_window" id="provider_action">
			<field name="name">Provider</field>
			<field name="res_model">provider.provider</field>
			<field name="view_mode">tree,form</field>
		</record>

		<record model="ir.ui.view" id="object_list">
			<field name="name">object List</field>
			<field name="model">object.object</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" />
					<field name="object_type" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="object_form">
			<field name="name">object Form</field>
			<field name="model">object.object</field>
			<field name="arch" type="xml">
				<form name="object Form">
					<sheet>
						<group>
							<field name="name" />
							<field name="object_type" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="object_kanban">
			<field name="name">object kanban</field>
			<field name="model">object.object</field>
			<field name="arch" type="xml">
				<kanban class="o_res_partner_kanban">
					<templates>
						<t t-name="kanban-box">
							<div t-attf-class="oe_kanban_global_click">
								<div t-attf-class="oe_kanban_content">
									Object Name : <field name="name" /><br/>
									Object Type : <field name="object_type" />
								</div>
							</div>
						</t>
					</templates>
				</kanban>
			</field>
		</record>

		<record model="ir.actions.act_window" id="object_action">
			<field name="name">object</field>
			<field name="res_model">object.object</field>
			<field name="view_mode">tree,form,kanban</field>
		</record>

		<record model="ir.ui.view" id="inquiry_list">
			<field name="name">inquiry List</field>
			<field name="model">inquiry.inquiry</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" />
					<field name="object_id" />
					<field name="attachment" />
					<field name="cust_id" />
					<field name="provider_id" />
					<field name="location" />
					<field name="remark" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="inquiry_form">
			<field name="name">inquiry Form</field>
			<field name="model">inquiry.inquiry</field>
			<field name="arch" type="xml">
				<form name="inquiry Form">
					<sheet>
						<group>
							<group>
								<field name="name" />
								<field name="object_id" />
								<field name="attachment" widget="attachment" />
								<field name="cust_id" />
								<field name="provider_id" />
								<field name="location" />
								<field name="remark" />
							</group>
							<group>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="inquiry_kanban">
			<field name="name">inquiry kanban</field>
			<field name="model">inquiry.inquiry</field>
			<field name="arch" type="xml">
				<kanban class="o_res_partner_kanban">
					<templates>
						<t t-name="kanban-box">
							<div t-attf-class="oe_kanban_global_click">
								<div t-attf-class="oe_kanban_content">
									Inquiry Name : <field name="name" /><br/>
									Object Name : <field name="object_id" /><br/>
									attachment : <field name="attachment" /><br/>
									Provider Name : <field name="provider_id" /><br/>
									Delivery Location : <field name="location" /><br/>
									Remark : <field name="remark" />
								</div>
							</div>
						</t>
					</templates>
				</kanban>
			</field>
		</record>

		<record model="ir.actions.act_window" id="inquiry_action">
			<field name="name">Inquiry</field>
			<field name="res_model">inquiry.inquiry</field>
			<field name="view_mode">tree,form,kanban</field>
		</record>

		<record model="ir.ui.view" id="order_list">
			<field name="name">Order List</field>
			<field name="model">order.order</field>
			<field name="arch" type="xml">
				<tree>
					<field name="inquiry_id" />
					<field name="object_id" />
					<field name="attachment" />
					<field name="start" />
					<field name="end" />
					<field name="price" />
					<field name="cust_id" />
					<field name="location" />
					<field name="state" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="order_form">
			<field name="name">Order Form</field>
			<field name="model">order.order</field>
			<field name="arch" type="xml">
				<form name="Order Form">
					<header>
						<button name="action_pending" states="progress,complated" string="Pending" type="object" class="btn-denger"/>
						<button name="action_progress" states="pending,complated" string="In Progress" type="object" class="btn-dark"/>
						<button name="action_complated" states="pending,progress" string="Complated" type="object" class="btn-success"/>
						<field name="state" widget="statusbar" statusbar_visible="pending,progress,complated" clickble="True" />
					</header>
					<sheet>
						<group>
							<group>
								<field name="inquiry_id" />
							</group>
							<group>
							</group>
						</group>
						<group>
							<group>
								<field name="object_id" />
								<field name="location" />
								<field name="attachment" />
							</group>
							<group>
								<field name="cust_id" />
								<field name="provider_id" />
								<field name="remark" />
							</group>
						</group>
						<notebook>
							<page string="Order">
							<group>
								<group>
									<field name="start" />
									<field name="end" />
									<field name="price" />
								</group>
								<group>
								</group>
							</group>
							</page>
						</notebook>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="order_kanban">
			<field name="name">Order kanban</field>
			<field name="model">order.order</field>
			<field name="arch" type="xml">
				<kanban class="o_res_partner_kanban">
					<templates>
						<t t-name="kanban-box">
							<div t-attf-class="oe_kanban_global_click">
								<div t-attf-class="oe_kanban_content">
									Inquiry Name : <field name="inquiry_id" /><br/>
									Object Name : <field name="object_id" /><br/>
									Attachment : <field name="attachment" /><br/>
									Start Printing : <field name="start" /><br/>
									End Printing : <field name="end" /><br/>
									Price : <field name="price" /><br/>
									Customer Name : <field name="cust_id" /><br/>
									Delivery Location : <field name="location" /><br/>
									Status : <field name="state" />
								</div>
							</div>
						</t>
					</templates>
				</kanban>
			</field>
		</record>

		<record model="ir.ui.view" id="order_graph">
			<field name="name">Order Garph</field>
			<field name="model">order.order</field>
			<field name="arch" type="xml">
				<graph>
					<field name="state" />
				</graph>
			</field>
		</record>

		<record model="ir.ui.view" id="order_pivot">
			<field name="name">Order Pivot</field>
			<field name="model">order.order</field>
			<field name="arch" type="xml">
				<pivot>
					<field name="object_id" type="row" />
					<field name="state" type="col" />
				</pivot>
			</field>
		</record>

		<record model="ir.actions.act_window" id="order_action">
			<field name="name">Order</field>
			<field name="res_model">order.order</field>
			<field name="view_mode">tree,form,kanban,graph,pivot</field>
		</record>

		<record model="ir.ui.view" id="order_pivot">
			<field name="name">Order Dashboard</field>
			<field name="model">order.order</field>
			<field name="arch" type="xml">
				<dashboard>
					<view type="pivot" ref="order_pivot" />
					<view type="graph" ref="order_graph" />
				</dashboard>
			</field>
		</record>

		<record model="ir.actions.act_window" id="dashboard_action">
			<field name="name">Dashboard</field>
			<field name="res_model">order.order</field>
			<field name="view_mode">dashboard,graph,pivot</field>
		</record>

		<menuitem name="Print Service" id="menuitem_rootmenu" web_icon="print_service,static/description/icon1.png" />
			<!-- <menuitem name="Registration" id="menuitem_menu_registration" parent="menuitem_rootmenu" sequence="1"/> -->
			<menuitem name="Users" id="menuitem_menu_user" parent="menuitem_rootmenu" action="user_action" sequence="1" />
			<menuitem name="Job Workers" id="menuitem_menu_provider" parent="menuitem_rootmenu" action="provider_action" sequence="2" />
			<menuitem name="Inquiry" id="menuitem_menu_inquiry" parent="menuitem_rootmenu" action="inquiry_action" sequence="3"/>
			<menuitem name="Order" id="menuitem_menu_order" parent="menuitem_rootmenu" action="order_action" sequence="4"/>
			<menuitem name="Reporting" id="menuitem_menu_reporting" parent="menuitem_rootmenu" sequence="5" groups="group_admin" />
				<menuitem name="Dashboard" id="submenu_dashboard" parent="menuitem_menu_reporting" action="dashboard_action" />
			<menuitem name="Configuration" id="menuitem_menu_configuration" parent="menuitem_rootmenu" sequence="6" groups="group_admin" />
				<menuitem name="Object" id="submenu_object" parent="menuitem_menu_configuration" action="object_action" />

	</data>
</odoo>