<odoo>
	<data>

		<record model="ir.ui.view" id="user_list">
			<field name="name">User List</field>
			<field name="model">user.user</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" />
					<field name="email" />
					<field name="mobile" />
					<field name="address" />
					<field name="city" />
					<field name="district" />
					<field name="pincode" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="user_form">
			<field name="name">User Form</field>
			<field name="model">user.user</field>
			<field name="arch" type="xml">
				<form name="User Form">
					<sheet>
						<group>
							<field name="name" />
							<field name="email" />
							<field name="mobile" />
							<field name="address" />
							<field name="city" />
							<field name="district" />
							<field name="pincode" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.actions.act_window" id="user_action">
			<field name="name">User</field>
			<field name="res_model">user.user</field>
			<field name="view_mode">tree,form</field>
		</record>

		<record model="ir.ui.view" id="provider_list">
			<field name="name">Provider List</field>
			<field name="model">provider.provider</field>
			<field name="arch" type="xml">
				<tree>
					<field name="owner_name" />
					<field name="name" />
					<field name="gst_number" />
					<field name="email" />
					<field name="mobile" />
					<field name="address" />
					<field name="city" />
					<field name="district" />
					<field name="pincode" />
					<field name="open_time" />
					<field name="closed_time" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="provider_form">
			<field name="name">Provider Form</field>
			<field name="model">provider.provider</field>
			<field name="arch" type="xml">
				<form name="Provider Form">
					<sheet>
						<group>
							<field name="owner_name" />
							<field name="name" />
							<field name="gst_number" />
							<field name="email" />
							<field name="mobile" />
							<field name="address" />
							<field name="city" />
							<field name="district" />
							<field name="pincode" />
							<field name="open_time" />
							<field name="closed_time" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.actions.act_window" id="provider_action">
			<field name="name">Provider</field>
			<field name="res_model">provider.provider</field>
			<field name="view_mode">tree,form</field>
		</record>

		<record model="ir.ui.view" id="object_list">
			<field name="name">object List</field>
			<field name="model">object.object</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" />
					<field name="object_type" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="object_form">
			<field name="name">object Form</field>
			<field name="model">object.object</field>
			<field name="arch" type="xml">
				<form name="object Form">
					<sheet>
						<group>
							<field name="name" />
							<field name="object_type" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="object_kanban">
			<field name="name">object kanban</field>
			<field name="model">object.object</field>
			<field name="arch" type="xml">
				<kanban class="o_res_partner_kanban">
					<templates>
						<t t-name="kanban-box">
							<div t-attf-class="oe_kanban_global_click">
								<div t-attf-class="oe_kanban_content">
									Object Name : <field name="name" /><br/>
									Object Type : <field name="object_type" />
								</div>
							</div>
						</t>
					</templates>
				</kanban>
			</field>
		</record>

		<record model="ir.actions.act_window" id="object_action">
			<field name="name">object</field>
			<field name="res_model">object.object</field>
			<field name="view_mode">tree,form,kanban</field>
		</record>

		<record model="ir.ui.view" id="inquiry_list">
			<field name="name">inquiry List</field>
			<field name="model">inquiry.inquiry</field>
			<field name="arch" type="xml">
				<tree>
					<field name="object_id" />
					<field name="attachment" />
					<field name="provider_id" />
					<field name="location" />
					<field name="remark" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="inquiry_form">
			<field name="name">inquiry Form</field>
			<field name="model">inquiry.inquiry</field>
			<field name="arch" type="xml">
				<form name="inquiry Form">
					<sheet>
						<group>
							<field name="object_id" />
							<field name="attachment" />
							<field name="provider_id" />
							<field name="location" />
							<field name="remark" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="inquiry_kanban">
			<field name="name">inquiry kanban</field>
			<field name="model">inquiry.inquiry</field>
			<field name="arch" type="xml">
				<kanban class="o_res_partner_kanban">
					<templates>
						<t t-name="kanban-box">
							<div t-attf-class="oe_kanban_global_click">
								<div t-attf-class="oe_kanban_content">
									Object Name : <field name="object_id" /><br/>
									Attachment : <field name="attachment" /><br/>
									Provider Name : <field name="provider_id" /><br/>
									Delivery Location : <field name="location" /><br/>
									Remark : <field name="remark" />
								</div>
							</div>
						</t>
					</templates>
				</kanban>
			</field>
		</record>

		<record model="ir.actions.act_window" id="inquiry_action">
			<field name="name">inquiry</field>
			<field name="res_model">inquiry.inquiry</field>
			<field name="view_mode">tree,form,kanban</field>
		</record>

		<record model="ir.ui.view" id="jobwork_list">
			<field name="name">JobWork List</field>
			<field name="model">jobwork.jobwork</field>
			<field name="arch" type="xml">
				<tree>
					<field name="inquiry_id" />
					<field name="attachment" />
					<field name="start" />
					<field name="end" />
					<field name="price" />
					<field name="customer_id" />
					<field name="location" />
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="jobwork_form">
			<field name="name">JobWork Form</field>
			<field name="model">jobwork.jobwork</field>
			<field name="arch" type="xml">
				<form name="JobWork Form">
					<sheet>
						<group>
						<group>
							<field name="inquiry_id" />
							<field name="start" />
							<field name="end" />
							<field name="price" />
							<field name="customer_id" />
						</group>
						<group>
							<field name="location" />
							<field name="attachment" />
							<field name="provider_id" />
							<field name="remark" />
						</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="jobwork_kanban">
			<field name="name">JobWork kanban</field>
			<field name="model">jobwork.jobwork</field>
			<field name="arch" type="xml">
				<kanban class="o_res_partner_kanban">
					<templates>
						<t t-name="kanban-box">
							<div t-attf-class="oe_kanban_global_click">
								<div t-attf-class="oe_kanban_content">
									Inquiry Name : <field name="inquiry_id" /><br/>
									Attachment : <field name="attachment" /><br/>
									Start Printing : <field name="start" /><br/>
									End Printing : <field name="end" /><br/>
									Price : <field name="price" /><br/>
									Customer Name : <field name="customer_id" /><br/>
									Delivery Location : <field name="location" />
								</div>
							</div>
						</t>
					</templates>
				</kanban>
			</field>
		</record>

		<record model="ir.ui.view" id="jobwork_graph">
			<field name="name">JobWork Garph</field>
			<field name="model">jobwork.jobwork</field>
			<field name="arch" type="xml">
				<graph>
					<field name="inquiry_id" />
				</graph>
			</field>
		</record>

		<record model="ir.actions.act_window" id="jobwork_action">
			<field name="name">JobWork</field>
			<field name="res_model">jobwork.jobwork</field>
			<field name="view_mode">tree,form,kanban,graph</field>
		</record>

		<menuitem name="Print Service" id="menuitem_rootmenu" web_icon="bhavin_training_print_solution,static/description/icon1.png" />
			<menuitem name="Registration" id="menuitem_menu_registration" parent="menuitem_rootmenu" sequence="1"/>
				<menuitem name="User" id="menuitem_menu_user" parent="menuitem_menu_registration" action="user_action" />
				<menuitem name="Provider" id="menuitem_menu_provider" parent="menuitem_menu_registration" action="provider_action" />
			<menuitem name="Inquiry" id="menuitem_menu_inquiry" parent="menuitem_rootmenu" sequence="2"/>
				<menuitem name="Inquiry" id="submenu_inquiry" parent="menuitem_menu_inquiry" action="inquiry_action" />
			<menuitem name="JobWork" id="menuitem_menu_jobwork" parent="menuitem_rootmenu" sequence="3"/>
				<menuitem name="JobWork" id="submenu_jobwork" parent="menuitem_menu_jobwork" action="jobwork_action" />
			<menuitem name="Report" id="menuitem_menu_report" parent="menuitem_rootmenu" sequence="4"/>
				<menuitem name="Dashboard" id="submenu_dashboard" parent="menuitem_menu_report" />
			<menuitem name="Configuration" id="menuitem_menu_configuration" parent="menuitem_rootmenu" sequence="5"/>
				<menuitem name="Object" id="submenu_object" parent="menuitem_menu_configuration" action="object_action" />

	</data>
</odoo>