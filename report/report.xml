<odoo>
	<template id="quotation_report">
		<t t-call="web.html_container">
			<t t-call="web.external_layout">
				<t t-foreach="docs" t-as="doc">
					<div class="page">
						<h2>Quotation Details</h2>
						<p>Quotation name : <span t-field="doc.name"/></p>
						<p>Page Size : <span t-field="doc.page_size"/></p>
						<p>Start Date : <span t-field="doc.start_date"/></p>
						<p>End Date : <span t-field="doc.end_date"/></p>
						<p>State : <span t-field="doc.state"/></p>
					</div>
				</t>
			</t>
		</t>
	</template>

	<report
		id="action_quotation_report"
		model="quotation.quotation"
		string="Print Quotation"
		report_type="qweb-pdf"
		name="bhavin_training_print_solution.quotation_report"
		file="bhavin_training_print_solution.quotation_report"/>
</odoo>